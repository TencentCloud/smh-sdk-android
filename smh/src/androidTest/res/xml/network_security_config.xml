<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!--debug模式才信任user证书-->
    <debug-overrides>
        <trust-anchors>
            <certificates src="user" />
        </trust-anchors>
    </debug-overrides>
    <base-config cleartextTrafficPermitted="true"/>
</network-security-config>